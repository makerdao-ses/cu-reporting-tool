{
  "name": "makerdao-cu-reporting-tool",
  "version": "1.1.0",
  "license": "AGPL-3.0",
  "description": "Tool for Core Unit administrators to push their monthly reports to the Ecosystem Performance API.",
  "private": true,
  "scripts": {
    "watch": "babel --watch src --out-dir build --extensions \".ts,.tsx\"",
    "build": "babel src --out-dir build --extensions \".ts,.tsx\"",
    "tsc": "tsc",
    "start": "npm run build && node ./build/main.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "typescript": "^4.8.3"
  }
}
